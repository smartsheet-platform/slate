	"message": "SUCCESS",
	"resultCode": 0,
	"result": {
		"id": 2331373580117892,
		"name": "API-created Group",
		"description": "Group created via API",
		"owner": "john.doe@smartsheet.com",
		"ownerId": 4583173393803140,
		"members": [{
			"id": 4583173393803140,
			"email": "john.doe@smartsheet.com",
			"firstName": "John",
